{
    "name": "cmobi/microservice-framework-bundle",
    "description": "Bundle that improve Symfony to work with microservices architecture",
    "type": "symfony-bundle",
    "license": "proprietary",
    "authors": [
        {
            "name": "Daniel Oliveira",
            "email": "daniel.oliveira@conta.mobi"
        }
    ],
    "autoload": {
        "psr-4": {
            "Cmobi\\MicroserviceFrameworkBundle\\": ""
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.6.1",
        "symfony/http-kernel": "2.8.*",
        "symfony/console": "^2.8",
        "monolog/monolog": "^1.19",
        "cmobi/rabbitmq-bundle": "^1.0.0",
        "symfony/process": "^2.8",
        "leaseweb/memcache-bundle": "^2.1"
    },
    "require-dev": {
        "symfony/framework-bundle": "~2.8",
        "phpunit/phpunit": "5.0",
        "satooshi/php-coveralls": "^1.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
